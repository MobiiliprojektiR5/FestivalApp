<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FestaritActivity">

    <LinearLayout
        android:layout_width="330dp"
        android:layout_height="640dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/RockfestBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/rockfest_2023" />

        <Button
            android:id="@+id/HankoBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/hanko_sommarfest" />

        <Button
            android:id="@+id/NummirockBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/nummirock_metal_festival" />

        <Button
            android:id="@+id/HimosBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/himos_juhannus_2023" />

        <Button
            android:id="@+id/IskelmaBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/iskelm_festivaali" />

        <Button
            android:id="@+id/ProvinssiBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/provinssi_2023" />

        <Button
            android:id="@+id/TuskaBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tuska_festival" />

        <Button
            android:id="@+id/RuisBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ruisrock_2023" />

        <Button
            android:id="@+id/SuomipopBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/suomi_pop_festivaalit" />

        <Button
            android:id="@+id/QstockBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/qstock_2023" />

        <Button
            android:id="@+id/KotkanBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/kotkan_merip_iv_t" />

        <Button
            android:id="@+id/FlowBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/flow_festival" />

        <Button
            android:id="@+id/BlockBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/blockfest_2023" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>